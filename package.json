{
  "name": "zui-react",
  "version": "0.0.5",
  "description": "zui in react version",
  "main": "./src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack-config/dev.js --progress",
    "build:umd": "cross-env NODE_ENV=production webpack --config webpack-config/umd.js",
    "build:babel": "cross-env NODE_ENV=production babel ./src --out-dir ./build --ignore spec.js,test/*.js",
    "build:copy": "babel-node ./script/copy.js",
    "build": "npm run build:umd && npm run build:babel && npm run build:copy",
    "pub": "npm run build && npm publish ./build"
  },
  "peerDependencies": {
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-tap-event-plugin": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisHchen/zui-react"
  },
  "keywords": [
    "react",
    "react-component",
    "zui"
  ],
  "author": "chris H chen",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "keycode": "^2.1.9",
    "lodash.throttle": "^4.1.1",
    "prop-types": "^15.5.10",
    "react-event-listener": "^0.4.5",
    "react-transition-group": "^1.2.0",
    "recompose": "^0.23.5",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "eslint": "^4.0.0",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "glob": "^7.1.2",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-reporter": "^4.0.0",
    "precss": "^1.4.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-tap-event-plugin": "^2.0.1",
    "style-loader": "^0.18.2",
    "stylelint": "^7.11.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
